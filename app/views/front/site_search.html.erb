<div class="ipacro-postcontent ipacro-postcontent-0 clearfix">
  <div class="ipacro-content-layout">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width:100%;height:140px;background-image:url(/images/join_us.jpg);background-size:100%;" >
      <h1 class="ipacro-headline" style="left:20px;position:relative;border-radius:10px;background:url('../images/nav.png');width:95%;transform:skew(-12deg);line-height:51px;text-indent:15px">搜索结果</h1>
      </div>
    </div>
  </div>
  <div class="ipacro-content-layout" style="margin-top:10px;min-height:400px;">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0">
        <div class="layout-item-2" style="min-height:400px">
          <ul>
          <% @search_result.each do |items| %>
            <% items.each do |item| %>
            <li>
              <%= link_to item.title, item %>
            </li>
            <% end %>
          <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>