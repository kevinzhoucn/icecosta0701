<div class="ipacro-postcontent ipacro-postcontent-0 clearfix">
  <div class="ipacro-content-layout">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width:100%;height:140px;background-image:url(http://tigermed.net/images/zx.jpg);background-size:100%;" >
      </div>
    </div>
  </div>
    <style type="text/css">
      @import url('/css/tabbing.css');
    </style>
    <script type="text/javascript" src="/js/tabbing.js"></script>
  <div class="ipacro-content-layout">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width: 25%" >
        <ul style="margin:1em 0 1em 0;">
        <% @news.each do |news| %>
          <li id="label<%=news.news_type%>"><%=news.news_type%></li>
        <%end%>
        </ul>
      </div>
      <div class="ipacro-layout-cell layout-item-0" style="width:75%" id="tabs">
        <% @news.each do |news| %>
          <div id="content<%=news.news_type%>" class="layout-item-2" style="margin-top:1em;min-height:300px">
            <%=news.news_type%>
          </div>
        <%end%>
      </div>
    </div>
  </div>
</div>