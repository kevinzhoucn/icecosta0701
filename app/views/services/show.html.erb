<div class="ipacro-postcontent ipacro-postcontent-0 clearfix">
  <div class="ipacro-content-layout">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width:100%;height:140px;background-image:url(/images/services.jpg);background-size:100%;" >
      <h1 class="ipacro-headline" style="left:20px;position:relative;border-radius:10px;background:url('/images/nav.png');width:95%;transform:skew(-12deg);line-height:51px;text-indent:15px"><%=t('services')%></h1>
      </div>
    </div>
  </div>
  <div class="ipacro-content-layout" style="margin-top:10px;">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width: 25%" >
        <%= render partial:'common/services/service_left'%>
      </div>
      <div class="ipacro-layout-cell layout-item-0" style="width: 75%" >
        <div class="layout-item-2" style="min-height:350px">
          <%= @service.content.html_safe %>
        </div>
      </div>
    </div>
  </div>
  <%= render partial:'common/misc/totop'%>
</div>