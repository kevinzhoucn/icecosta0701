<div>
  <h2 class="header">k 网站参数配置</h2>
  <div class="monitor">
    <div class="clearfix">
      <div style="min-height:40px;line-height:40px;"> 
        <li style="margin:0 30px;float:left;font-family:icomoon">k 网站参数列表</li>
      </div>
      <div style="border:1px solid rgb(235,235,235);margin: 10px 40px;border-radius:5px;height:auto;overflow:hidden">
        <ul style="width:100%">
        <% @cpanel_site_configs_other.each do |cpanel_site_config| %>
          <li>
            <%= cpanel_site_config.key %>
            <span style="position: absolute;right:340px">
              <%= link_to '编辑', edit_cpanel_site_config_path(cpanel_site_config),class:'submitbutton' %>
            </span>
          </li>
        <%end%>
        </ul>
      </div>
    </div>
  </div>
</div>