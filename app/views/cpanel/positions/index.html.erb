<%= link_to 'New Position', new_cpanel_position_path %>

 <div class="ipacro-postcontent ipacro-postcontent-0 clearfix">
  <div class="ipacro-content-layout">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width:100%;height:140px;background-image:url(http://tigermed.net/images/2_01.jpg);background-size:100%;">
      </div>
    </div>
  </div>
  <div class="ipacro-content-layout">
    <div class="ipacro-content-layout-row">
      <div class="ipacro-layout-cell layout-item-0" style="width:25%">
        <section style="margin-top:1em">
        <form action="http://baidu.com/s?site%3Abrivates.com%20" method="post">
          <input type="search" placeholder="搜索职位" result="8">
          <a class="ipacro-button">搜索...</a>
        </form>
        </section>
      </div>
      <div class="ipacro-layout-cell layout-item-0" style="width:75%">
        <div class="layout-item-2" style="margin-top:1em;min-height:300px">
          <ul>
          <% @positions.each do |position| %>
            <li><%= link_to position.title, position %> <%=position.position_type%></li>

            <td><%= link_to 'Show', cpanel_position_path(position) %></td>
          <td><%= link_to 'Edit', edit_cpanel_position_path(position) %></td>
          <td><%= link_to 'Destroy', cpanel_position_path(position), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>